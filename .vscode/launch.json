    "version": "0.2.0",

    // debug mp4-decoder
    "configurations": [
        {
            "type": "cppdbg",
            "request": "launch",
            "name": "Debug mp4-decoder",
            "program": "${workspaceFolder}/mp4-decoder",
            "args": ["sample.mp4"],
            "cwd": "${workspaceFolder}",
            "MIMode": "gdb",
            "miDebuggerPath": "gdb",
            "stopAtEntry":false
        }
        
    ]
}